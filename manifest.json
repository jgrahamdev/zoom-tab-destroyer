{
  "name": "Zoom Tab Destroyer",
  "version": "0.1",
  "description": "Close Zoom tabs once the meeting has started.",
  "manifest_version": 2,
  "permissions": [
    "webNavigation",
    "https://*.zoom.us/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
